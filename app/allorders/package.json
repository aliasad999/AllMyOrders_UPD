{
    "name": "allorders",
    "version": "0.0.1",
    "description": "all My orders app",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "scripts": {
        "deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
        "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateCachebusterInfo && cp xs-app.json dist/xs-app.json",
        "start:local": "ui5 serve --config ui5-local.yaml --open index.html"
    },
    "devDependencies": {
        "@ui5/cli": "^3.0.0",
        "@sap/ux-ui5-tooling": "1",
        "@sap-ux/ui5-middleware-fe-mockserver": "2",
        "@sap/ui5-builder-webide-extension": "^1.1.8",
        "ui5-task-zipper": "^0.5.0",
        "mbt": "^1.2.18"
    },
    "ui5": {
        "dependencies": [
            "@sap/ui5-builder-webide-extension",
            "ui5-task-zipper",
            "ui5-middleware-simpleproxy",
            "mbt"
        ]
    },
    "dependencies": {
      
    }
}
