{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/v2/(.*)",
      "target": "/v2/$1",
      "destination": "srv-api",
      "authenticationType": "xsuaa"
  },
  {
    "source": "^/srv(.*)",
    "destination": "srv-api",
    "authenticationType": "xsuaa"
},
  {
    "source": "^/sap/(.*)$",
    "target": "/sap/$1",
    "destination": "SAP_GATEWAY",
    "authenticationType": "none",
    "csrfProtection": false
  },
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}
