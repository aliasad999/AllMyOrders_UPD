{
    "_version": "1.49.0",
    "sap.app": {
        "id": "ordermonitoring.allmyorders",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.9.4",
            "toolsId": "973a08f7-7cf9-42f4-9108-2c6e4532798e"
        },
        "dataSources": {
            "mainService": {
                "uri": "/v2/srv-open-orders/",
                "type": "OData",
                "settings": {
                    "annotations": [
                        "annotations"
                    ],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "notes": {
                "uri": "/sap/opu/odata/sap/LORD_ODATA_ORDER_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "annotations": {
				"type": "ODataAnnotation",
				"uri": "annotations.xml",
				"settings": {
					"localUri": "annotations.xml"
				}
			}
        },
        "crossNavigation": {
            "inbounds": {
                "ordermonitoring-allmyorders-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "allmyorders",
                    "action": "display",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": "sap-icon://sales-order",
                    "indicatorDataSource": {
                        "dataSource": "mainService",
                        "path": "/v2/srv-open-orders/Results/$count",
                        "refresh": 10
                    }
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.113.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "ordermonitoring.allmyorders.i18n.i18n"
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "notesModel": {
                "dataSource": "notes",
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "useBatch": false
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultOperationMode":"Server"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "ordermonitoring.allmyorders.view",
                "controlAggregation": "pages",
                "controlId": "idAppControl",
                "clearControlAggregation": false
            },
            "routes": [
              {
                "pattern": "",
                "name": "SalesOrders",
                "view": "OrderList",
                "viewId": "OrderList"
            }
            ],
            "targets": {}
        },
        "rootView": {
            "viewName": "ordermonitoring.allmyorders.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    }
}